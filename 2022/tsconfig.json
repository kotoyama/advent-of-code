{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "strict": true,
    "strictNullChecks": false,
    "module": "esnext",
    "lib": ["ES2019"],
    "target": "es2019",
    "moduleResolution": "node",
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
